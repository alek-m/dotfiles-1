# msmtp config file

# gmail
account gmail 
host smtp.gmail.com
port 587
protocol smtp
auth on
from @gmail.com
user @gmail.com
password  
tls on
tls_nocertcheck

# gmx
account gmx
host mail.gmx.com
port 587
protocol smtp
auth on
from @gmx.com
user @gmx.com
password 
tls on
tls_nocertcheck

# use gmail as default
account default : gmail
