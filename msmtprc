# vim: ft=msmtp

account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from pbrisbin@gmail.com
user pbrisbin@gmail.com
passwordeval getnetrc pbrisbin@gmail.com
tls on
tls_nocertcheck

account thoughtbot
host smtp.gmail.com
port 587
protocol smtp
auth on
from pat@thoughtbot.com
user pat@thoughtbot.com
passwordeval getnetrc pat@thoughtbot.com
tls on
tls_nocertcheck

account default : gmail
