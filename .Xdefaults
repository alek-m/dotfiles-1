! Colors
#include "/home/patrick/.xcolors/jasonwryan.xcolors"

! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull

! }}}

! Urxvt {{{
URxvt*font:     -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt*scrollBar:  false
URxvt*jumpScroll: true
URxvt*skipScroll: true
URxvt*visualBell: false

URxvt*depth:       32
URxvt*buffered:    true
URxvt*background:  rgba:0000/0000/0000/dddd
URxvt*transparent: false
URxvt*fading:      5%
URxvt*fadeColor:   grey
URxvt*shading:     75

URxvt*intensityStyles: true
URxvt*termName:        rxvt-256color
URxvt*urgentOnBell:    true

! URLs in rxvt
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher:     /usr/bin/chromium
URxvt*keysym.C-o:      perl:matcher
URxvt*matcher.button:  1

! }}}

! Xterm {{{
XTerm*font:         -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
XTerm*boldFont:     -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
XTerm*scrollBar:    false
XTerm*termName:     xterm-256color
XTerm*bellIsUrgent: true

xterm*font:     -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
xterm*boldFont: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*

! }}}
